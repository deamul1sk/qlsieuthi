<div class="row">
        <div class="col-md-12">
            <!-- BEGIN EXAMPLE TABLE PORTLET-->
            <div class="portlet box grey-cascade">
                <div class="portlet-title">
                    <div class="caption">
                        <i class="fa fa-globe"></i>{{ 'SIDEBAR.Directory' | translate }} > {{ 'SIDEBAR.Currency' | translate }} > {{ 'Action.Detail'
                        | translate }}
                    </div>
                </div>
                <div class="portlet-body">
                    <div class="row">
                        <div class="col-md-12">
                            <form class="form-horizontal align-middle">
                                <div class="row">
                                    <div class="col-md-8">
                                        <div class="form-group">
                                            <label class="col-md-4 text-right">{{'Currency.CurrencyCode'|translate}}</label>
                                            <div class="col-md-8">{{currency?.currencyCode}}</div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-4 text-right">{{'Currency.CurrencyName'|translate}}</label>
                                            <div class="col-md-8">{{currency?.currencyName}}</div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-4 text-right">{{'Country.Title'|translate}}</label>
                                            <div class="col-md-8">{{currency?.country.countryName}}</div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-4 text-right">{{'Currency.Modifier'|translate}}</label>
                                            <div class="col-md-8">{{currency?.modifier}}</div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-4 text-right">{{'Currency.Modified'|translate}}</label>
                                            <div class="col-md-8">{{currency?.modified}}</div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-4 text-right">{{'Currency.Version'|translate}}</label>
                                            <div class="col-md-8">{{currency?.version}}</div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-4 text-right">{{'Currency.Status'|translate}}</label>
                                            <div class="col-md-8">{{currency?.status==1?'Active':'Inactive'}}</div>
                                        </div>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn default" (click)="goBack()">
                                        <i class="fa fa-undo" aria-hidden="true"></i>{{ 'Action.Back' | translate }}</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <!-- END EXAMPLE TABLE PORTLET-->
        </div>
    </div>